{
  "name": "vwo-elite",
  "private": true,
  "version": "1.0.0",
  "description": "Premium VWO Exam Preparation Lab",
  "author": "Jamel",
  "type": "module",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "concurrently \"electron-vite dev\" \"pnpm run proxy:somtoday\"",
    "dev:all": "pnpm -r --parallel dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=10240 electron-vite build",
    "build:all": "pnpm -r build",
    "build:final": "node scripts/build-elite-v21.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "proxy:somtoday": "node server/somtoday-proxy.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "node patch_kapsule.cjs",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force node_modules, .next, dist, build, out, coverage, .tsbuildinfo, package-lock.json, pnpm-lock.yaml, .vite -ErrorAction SilentlyContinue\"",
    "reinstall": "pnpm run clean && pnpm install",
    "audit:clean": "powershell -Command \"Remove-Item -Recurse -Force node_modules, out, dist, resources/bin, .vite -ErrorAction SilentlyContinue\"",
    "audit:install": "pnpm install --frozen-lockfile",
    "audit:native": "npx electron-builder install-app-deps",
    "audit:types": "tsc -b apps/main apps/preload apps/renderer",
    "audit:lint": "eslint . --max-warnings 75",
    "audit:arch": "depcruise src apps packages --config .dependency-cruiser.cjs",
    "audit:security": "pnpm audit --audit-level=high",
    "audit:test": "vitest run --bail 1",
    "audit:test:smoke": "node scripts/smoke-test.js",
    "audit:build": "pnpm run build && npx electron-builder build --win --publish never",
    "audit:verify-asar": "node scripts/verify-asar.js",
    "audit:assets": "node scripts/check-assets.js",
    "audit:ipc": "node scripts/check-ipc-channels.js",
    "pre-deploy:elite": "pnpm run audit:clean && pnpm run audit:install && pnpm run audit:native && pnpm run audit:types && pnpm run audit:lint && pnpm run audit:arch && pnpm run audit:assets && pnpm run audit:ipc && pnpm run audit:test && pnpm run audit:build && pnpm run audit:verify-asar"
  },
  "build": {
    "appId": "com.vwo.elite",
    "productName": "VWO Elite",
    "copyright": "Copyright Â© 2026 Jamel",
    "asar": true,
    "compression": "store",
    "extraResources": [
      {
        "from": "public/splash.html",
        "to": "splash.html"
      },
      {
        "from": "public/splash_cinematic.html",
        "to": "splash_cinematic.html"
      },
      {
        "from": "public/Installer",
        "to": "Installer"
      },
      {
        "from": "public/favicon.ico",
        "to": "favicon.ico"
      }
    ],
    "asarUnpack": [
      "**/*.node",
      "**/node_modules/better-sqlite3/**",
      "**/node_modules/vectordb/**",
      "**/node_modules/@lancedb/**"
    ],
    "directories": {
      "output": "dist/electron",
      "buildResources": "public"
    },
    "win": {
      "target": "nsis",
      "icon": "public/android-chrome-512x512.png"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowElevation": false,
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "VWO Elite",
      "include": "build/installer.nsh"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/android-chrome-512x512.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/android-chrome-512x512.png"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
